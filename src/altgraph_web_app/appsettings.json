{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Redis": {
    "ConnectionString": "",
    "SSL": true
  },
  "Cosmos": {
    "ConnectionString": "",
    "DatabaseId": "dev",
    "PopulateQueryMetrics": true,
    "QueryMetricsEnabled": true,
    "MaxDegreeOfParallelism": 1,
    "UseAzureADAuthentication": false,
    "PreferredLocations": [""]
  },
  "KeyVault": {
    "UseKeyVault": false,
    "KeyVaultUri": ""
  },
  "Cache": {
    "Tenant": "123",
    "AppCacheMethod": "redis"
  },
  "NpmPaths": {
    "NodesCsvFile": "data/graph/nodes.csv",
    "EdgesCsvFile": "data/graph/edges.csv",
    "LibraryCacheFileTemplate": "data/cache/{0}.json",
    "TripleQueryStructCacheFile": "data/cache/TripleQueryStruct.json"
  },
  "ImdbPaths": {
    "ImdbRawNameBasicsFile": "data/imdb_raw/name.basics.tsv",
    "ImdbRawTitleBasicsFile": "data/imdb_raw/title.basics.tsv",
    "ImdbRawTitlePrincipalsFile": "data/imdb_raw/title.principals.tsv",
    "ImdbMoviesDocumentsFile": "data/imdb_refined/movies.json",
    "ImdbMoviesSeedFile": "data/imdb_refined/movies_seed.json",
    "ImdbMoviesMapFile": "data/imdb_refined/movies_map.json",
    "ImdbPeopleDocumentsFile": "data/imdb_refined/people.json",
    "ImdbPeopleMapFile": "data/imdb_refined/people_map.json",
    "ImdbMoviesOfInterestFile": "data/imdb_refined/movies_of_interest.txt",
    "ImdbPeopleOfInterestFile": "data/imdb_refined/people_of_interest.txt",
    "ImdbSmallTriplesDocumentsFile": "data/imdb_refined/sm_triples.json"
  },
  "Npm": {
    "ContainerName": "altgraph",
    "PartitionKey": "/pk",
    "DefaultTenant": "123"
  },
  "Imdb": {
    "GraphDomain": "imdb",
    "GraphSource": "cosmos",
    "GraphDirected": false,
    "SeedContainerName": "imdb_seed",
    "GraphContainerName": "imdb_graph",
    "PartitionKey": "/pk"
  }
}
